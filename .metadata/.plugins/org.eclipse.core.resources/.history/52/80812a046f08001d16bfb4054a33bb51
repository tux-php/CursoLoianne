package com.fernando.cursojava.aula25;

public class TesteCarro {

	public static void main(String[] args) {
		Carro van = new Carro();
		van.marca = "Fiat";
		van.modelo = "Ducato";
		van.numPassegeiros = 10;
		van.capCombustivel = 41;
		van.consumoCombustivel = 0.2;

		System.out.println(van.marca);
		System.out.println(van.modelo);

		System.out.println("A autonomia do carro Ã©: " + van.obterAutonomia());
		
		double combustivelUtilizado = van.calcularCombustivel(10);
		System.out.println("O carro kicks consumiu " + combustivelUtilizado + "em 10km.");

	}

}
