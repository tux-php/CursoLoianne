package com.fernando.cursojava.aula25;

public class Carro {
	String marca;
	String modelo;
	int numPassegeiros;
	double capCombustivel;
	double consumoCombustivel;

	double obterAutonomia() {
		System.out.println("O m√©todo obterAutonomia() foi chamado.");

		return capCombustivel * consumoCombustivel;
	}

}
